    "eslint-plugin-react-refresh": "^0.4.24",
    "fake-indexeddb": "^6.2.5",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.2",
    "firebase-functions-test": "^3.4.1",
    "globals": "^16.5.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^170.0.0",
    "idb": "^8.0.3",
    "javascript-obfuscator": "^4.2.2",
