{
  "name": "indii-os",
  "description": "The Operating System for Independent Artists",
  "author": "IndiiOS LLC",
  "private": true,
  "version": "0.1.0-beta.2",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:landing": "cd landing-page && npm install && npm run build",
    "build:studio": "vite build",
    "build:all": "npm run build:landing && npm run build:studio",
    "preview": "vite preview",
    "electron:dev": "npm run build:electron && VITE_DEV_SERVER_URL=http://localhost:4242 npx electron dist-electron/main.cjs",
    "build:electron": "npx esbuild electron/main.ts --bundle --platform=node --outfile=dist-electron/main.cjs --format=cjs --external:electron --external:keytar --external:fluent-ffmpeg --external:ffmpeg-static --external:ffprobe-static --external:electron-squirrel-startup --external:cpu-features --external:ssh2 --external:ssh2-sftp-client --external:*.node && npx esbuild electron/preload.ts --bundle --platform=node --outfile=dist-electron/preload.cjs --format=cjs --external:electron",
    "electron:build": "npm run build:electron && electron-builder --publish never",
    "desktop:dev": "npm run build:electron && VITE_DEV_SERVER_URL=http://localhost:4242 npx electron dist-electron/main.cjs",
    "build:desktop": "bash scripts/build-desktop.sh release all",
    "build:desktop:mac": "bash scripts/build-desktop.sh release mac",
    "build:desktop:win": "bash scripts/build-desktop.sh release win",
    "build:desktop:linux": "bash scripts/build-desktop.sh linux",
    "build:studio:mac": "vite build && electron-builder --config electron-builder-studio.json --publish never",
    "build:studio:win": "vite build && electron-builder --config electron-builder-studio.json --win --publish never",
    "build:studio:linux": "vite build && electron-builder --config electron-builder-studio.json --linux AppImage --publish never",
    "build:studio:all": "vite build && electron-builder --config electron-builder-studio.json -mlw --publish never",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "rebuild": "npx @electron/rebuild",
    "deploy": "npm run build:studio && firebase deploy --only hosting:app",
    "check:merge": "node scripts/check-merge-integrity.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@genkit-ai/ai": "^1.26.0",
    "@genkit-ai/express": "^1.26.0",
