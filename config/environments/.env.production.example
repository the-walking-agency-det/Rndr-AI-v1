# Environment Configuration - Production
# Copy this file to .env.production and fill in production values

# ========================================
# AUTHENTICATION & FIREBASE
# ========================================
VITE_FIREBASE_CONFIG={"apiKey":"your-prod-api-key","authDomain":"your-project.firebaseapp.com","projectId":"your-project-id","storageBucket":"your-project-id.appspot.com","messagingSenderId":"your-sender-id","appId":"your-app-id","measurementId":"your-measurement-id"}

# Firebase Functions (Backend)
VITE_VERTEX_PROJECT_ID=your-gcp-project-id
VITE_VERTEX_LOCATION=us-central1

# ========================================
# AI MODELS (Production)
# ========================================
VITE_GEMINI_API_KEY=your-production-gemini-api-key
VITE_GEMINI_API_KEY_PROD=your-production-gemini-api-key
VITE_GEMINI_API_KEY_PREPROD=your-pre-production-gemini-api-key
VITE_GEMINI_API_KEY_BETA=your-beta-gemini-api-key

# Vertex AI for expensive operations
VITE_ACCESS_TOKEN=your-vertex-access-token

# ========================================
# AGENT SYSTEM (Production)
# =====================================
VITE_AGENT_API_KEY=your-agent-api-key

# Browser Agent (Puppeteer-based) - Typically disabled in production
VITE_BROWSER_AGENT_ENABLED=false
VITE_BROWSER_AGENT_TIMEOUT=30000

# ========================================
# PLATFORM-SPECIFIC (Production)
# ========================================
# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your-maps-api-key

# Stripe (Production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-publishable-key

# Sentry (Error tracking - Production)
VITE_SENTRY_DSN=https://your-sentry-dsn

# ========================================
# FEATURE FLAGS (Production)
# =====================================
VITE_INSTRUMENT_DEBUG_MODE=false
VITE_AGENT_DEBUG_MODE=false
VITE_STRICT_MODE=true

# Enable/disable features
VITE_ENABLE_INSTRUMENTS=true
VITE_ENABLE_DOCKER=false
VITE_ENABLE_LOCAL_COMPUTE=false
VITE_ENABLE_BETA_FEATURES=false

VITE_ENABLE_PUBLICIST_AGENTS=true
VITE_ENABLE_BROWSER_AGENT=false
VITE_ENABLE_VIDEO_GENERATION=true
VITE_ENABLE_AUDIO_ANALYSIS=true

# ========================================
# ANALYTICS (Production)
# =====================================
VITE_ANALYTICS_ID=your-analytics-id

# ========================================
# PRODUCTION DEPLOYMENT
# =====================================
VITE_APP_URL=https://indiios-studio.web.app
VITE_API_URL=https://api.indiios-studio.web.app
VITE_CDN_URL=https://cdn.indiios-studio.web.app

# ========================================
# RATE LIMITS (Production)
# =====================================
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=60
VITE_RATE_LIMIT_BURST_LIMIT=100
