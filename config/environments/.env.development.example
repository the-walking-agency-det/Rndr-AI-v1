# Environment Configuration - Development
# Copy this file to .env.development and fill in your values

# ========================================
# AUTHENTICATION & FIREBASE
# ========================================
VITE_FIREBASE_CONFIG={"apiKey":"YOUR_API_KEY","authDomain":"your-project.firebaseapp.com","projectId":"your-project-id","storageBucket":"your-project-id.appspot.com","messagingSenderId":"your-sender-id","appId":"your-app-id","measurementId":"your-measurement-id"}

# Firebase Functions (Backend)
VITE_VERTEX_PROJECT_ID=your-gcp-project-id
VITE_VERTEX_LOCATION=us-central1

# ========================================
# AI MODELS
# ========================================
VITE_GEMINI_API_KEY=your-gemini-api-key
VITE_GEMINI_API_KEY_PROD=your-production-gemini-api-key
VITE_GEMINI_API_KEY_PREPROD=your-preprod-gemini-api-key
VITE_GEMINI_API_KEY_BETA=your-beta-gemini-api-key

# Vertex AI for expensive operations
VITE_ACCESS_TOKEN=your-vertex-access-token

# ========================================
# AGENT SYSTEM
# =====================================
VITE_AGENT_API_KEY=your-agent-api-key

# Browser Agent (Puppeteer-based)
VITE_BROWSER_AGENT_ENABLED=true
VITE_BROWSER_AGENT_TIMEOUT=30000

# ========================================
# PLATFORM-SPECIFIC
# ========================================
# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your-maps-api-key

# Dropbox (optional, for backup)
VITE_DROPBOX_API_KEY=your-dropbox-api-key

# Stripe (Production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-publishable-key

# ========================================
# DEVELOPMENT TOOLS
# =====================================
VITE_DEV_SERVER_URL=http://localhost:4242

# Enable/disable features during development
VITE_INSTRUMENT_DEBUG_MODE=true
VITE_AGENT_DEBUG_MODE=false
VITE_STRICT_MODE=true

# ========================================
# LOCAL SERVER URL FOR DESKTOP
# =====================================
# Used by Electron to communicate with dev server
VITE_DEV_SERVER_URL=http://localhost:4242
VITE_ELECTRON=true

# ========================================
# USAGE TRACKING (Optional)
# =====================================
VITE_ANALYTICS_ID=your-analytics-id
VITE_SENTRY_DSN=https://your-sentry-dsn

# ========================================
# FEATURE FLAGS
# =====================================
VITE_ENABLE_INSTRUMENTS=true
VITE_ENABLE_DOCKER=false
VITE_ENABLE_LOCAL_COMPUTE=false
VITE_ENABLE_BETA_FEATURES=false

VITE_ENABLE_PUBLICIST_AGENTS=true
VITE_ENABLE_BROWSER_AGENT=false
VITE_ENABLE_VIDEO_GENERATION=true
VITE_ENABLE_AUDIO_ANALYSIS=true

# ========================================
# TESTING
# =====================================
VITE_TEST_MOCK_FIREBASE=true
VITE_TEST_MOCK_AUTH=true
VITE_TEST_MOCK_SERVICES=false
